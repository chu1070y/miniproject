<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.salem.mapper.BoardAttachMapper">

	<insert id="insert">
		insert into attach (uuid, uploadpath, filename, filetype,bno)
		values(#{uuid},#{uploadPath},#{fileName},#{fileType},#{bno})
	</insert>
	
	<delete id="delete">
		delete from attach where uuid = #{uuid}
	</delete>
	
	<select id="findByBno" resultType="org.salem.domain.BoardAttachVO">
		select * from attach where bno = #{bno}
	</select>


</mapper>